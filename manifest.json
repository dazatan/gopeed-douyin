{
  "name": "douyin-downloader",
  "author": "gopeed-user",
  "title": "抖音视频下载器",
  "description": "解析并下载抖音无水印视频，支持多种链接格式",
  "icon": "icon.png",
  "version": "1.1.0",
  "homepage": "https://github.com/dazatan/gopeed-douyin",
  "scripts": [
    {
      "event": "onResolve",
      "match": {
        "urls": [
          "*://v.douyin.com/*",
          "*://www.douyin.com/video/*",
          "*://www.douyin.com/note/*",
          "*://www.iesdouyin.com/*",
          "*://douyin.com/*"
        ]
      },
      "entry": "dist/index.js"
    }
  ],
  "settings": [
    {
      "name": "timeout",
      "title": "请求超时时间(毫秒)",
      "description": "解析请求的超时时间",
      "type": "number",
      "default": 30000
    }
  ]
}
